<div class="calendar_container">
	<div id="calendar">
	</div>
</div>


<%= month_calendar events: @schedules do |date, schedules| %>

<%= date.day %>

<% schedules.each do |schedule| %>
<div>
	<strong><%= schedule.start_time.hour %></strong>
	<%= schedule.event %>
	<%= schedule.users.ids %>

</div>
<% end %>

<% end %>